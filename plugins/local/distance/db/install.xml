      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="aluno_discipllina" UNIQUE="true" FIELDS="aluno_id, disciplina_id"/>
